  "devDependencies": {
    "@pandacss/dev": "^0.52.0",
    "@pandacss/types": "^0.52.0",
    "@radix-ui/colors": "3.0.0",
    "@types/node": "^22.13.1",
    "outdent": "^0.8.0",
    "tsup": "^8.3.6"
  },
  "peerDependencies": {
    "@pandacss/dev": ">=0.52.0",
    "@radix-ui/colors": ">=3.0.0"
